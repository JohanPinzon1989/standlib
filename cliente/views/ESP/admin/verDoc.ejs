<% for(var count=0; count < results.length; count++) { %>
<div class="card">
  <div class="card-body">
    <div style="margin-top: 15px">
      <object
        data="/<%= results[count].Link %>"
        type="application/img"
        width="100%"
        height="100%"
      >
        <p>
          Unable to display PDF file.
          <a href="/<%= results[count].Link %>">Download</a> instead.
        </p>
      </object>
    </div>
  </div>
</div>
<% } %>
